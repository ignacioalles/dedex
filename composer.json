{
    "type": "symfony-bundle",
    "name": "miqwit/dedex",
    "description": "Parse DDEX ERN files and provide a convenient API to use it",
	  "keywords": ["ddex", "parsing", "music", "xml"],
	  "homepage": "https://github.com/miqwit/dedex",
	  "authors": [
        {
            "name": "MickaÃ«l Arcos",
            "role": "Developer"
        }
		],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "goetas-webservices/xsd2php-runtime": "^0.2.2",
        "symfony/console": "^2.7|^3.0|^4.0|^5.0",
        "symfony/dotenv": "^4.0|^5.0",
        "symfony/flex": "^1.18",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/http-kernel": "^4.0|^5.1.5",
        "symfony/yaml": "^2.2|^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "goetas-webservices/xsd2php": "^0.4.2",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": false
        }
    },
    "autoload": {
        "psr-4": {
            "DedexBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DedexBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {},
        "post-install-cmd": [],
        "post-update-cmd": []
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
